		<dc:title id="title">What Is Art?</dc:title>
		<meta property="file-as" refines="#title">What Is Art?</meta>
		<dc:subject id="subject-1">Arts -- Philosophy</dc:subject>
		<dc:subject id="subject-2">Arts and morals</dc:subject>
		<meta property="authority" refines="#subject-1">LCSH</meta>
		<meta property="term" refines="#subject-1">sh2009113914</meta>
		<meta property="authority" refines="#subject-2">LCSH</meta>
